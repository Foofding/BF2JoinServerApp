<Window x:Class="BF2JoinServerApp.RenameProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BF2JoinServerApp"
        mc:Ignorable="d"
        Title="RenameProfileWindow" Height="150" Width="250">
    <Grid>
        <TextBox x:Name="ProfileNameTextBox" Margin="10" VerticalAlignment="Top" HorizontalAlignment="Stretch" />
        <StackPanel Orientation="Horizontal">
            <Button Content="Cancel" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Height="30" Click="Button_Click" />
            <Button Content="Save" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Height="30" />
        </StackPanel>
        
    </Grid>
</Window>
